@page
@model Paqueteria.Pages.Dependencias.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Dependencia[0].NombreDpd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dependencia[0].DescripcionDpd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dependencia[0].StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dependencia[0].FinaltDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dependencia[0].Estados)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Dependencia) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreDpd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionDpd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FinaltDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estados.NombreEdo)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.DependenciaID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.DependenciaID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.DependenciaID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
